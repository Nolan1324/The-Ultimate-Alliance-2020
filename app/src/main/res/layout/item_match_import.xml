<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout android:id="@+id/match_import_item"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="horizontal"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:padding="5dp"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <View
        android:id="@+id/match_import_item_disabled"
        android:visibility="visible"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:foreground="#33aaaaaa" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:weightSum="9"
        android:background="?android:attr/selectableItemBackground">

        <TextView
            android:id="@+id/match_import_item_number"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:textSize="@dimen/list_text"
            android:textAlignment="center"
            android:textColor="?android:textColorPrimary"
            android:layout_gravity="center"
            android:layout_weight="1" />

        <TextView
            android:id="@+id/match_import_item_date"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:textSize="@dimen/list_text"
            android:textColor="?android:textColorPrimary"
            android:textAlignment="center"
            android:layout_gravity="center"
            android:layout_weight="2" />

        <TextView
            android:id="@+id/match_import_item_red1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_margin="5dp"
            android:textSize="@dimen/list_text"
            android:textAlignment="center"
            android:textColor="?android:textColorPrimary"
            android:background="@android:color/holo_red_light"
            android:layout_gravity="center"
            android:layout_weight="1" />

        <TextView
            android:id="@+id/match_import_item_red2"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_margin="5dp"
            android:textSize="@dimen/list_text"
            android:textAlignment="center"
            android:textColor="?android:textColorPrimary"
            android:background="@android:color/holo_red_light"
            android:layout_gravity="center"
            android:layout_weight="1" />

        <TextView
            android:id="@+id/match_import_item_red3"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_margin="5dp"
            android:textSize="@dimen/list_text"
            android:textAlignment="center"
            android:textColor="?android:textColorPrimary"
            android:background="@android:color/holo_red_light"
            android:layout_gravity="center"
            android:layout_weight="1" />

        <TextView
            android:id="@+id/match_import_item_blue1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_margin="5dp"
            android:textSize="@dimen/list_text"
            android:textAlignment="center"
            android:textColor="?android:textColorPrimary"
            android:background="@android:color/holo_blue_light"
            android:layout_gravity="center"
            android:layout_weight="1" />

        <TextView
            android:id="@+id/match_import_item_blue2"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_margin="5dp"
            android:textSize="@dimen/list_text"
            android:textAlignment="center"
            android:textColor="?android:textColorPrimary"
            android:background="@android:color/holo_blue_light"
            android:layout_gravity="center"
            android:layout_weight="1" />

        <TextView
            android:id="@+id/match_import_item_blue3"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_margin="5dp"
            android:textSize="@dimen/list_text"
            android:textAlignment="center"
            android:textColor="?android:textColorPrimary"
            android:background="@android:color/holo_blue_light"
            android:layout_gravity="center"
            android:layout_weight="1" />

    </LinearLayout>
</RelativeLayout>